<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis.EditCenter">

	<select id="selectForMapkeyGet" parameterType="java.util.Map" resultType="WorkOutDTO">
		SELECT NAME,address FROM H_MEMBER WHERE ID=#{ID}
	</select>

	<insert id="insertRegiCenter" parameterType="java.util.Map">
		insert into regicenter values(SEQ_RegiCenter_NO,#{id},#{mapkey},default)
	</insert>
	
	<select id="mapkeyIsIn" parameterType="java.util.Map" resultType="int">
		SELECT ISALLOWED FROM REGICENTER WHERE id=#{ID} and mapkey=#{mapkey}
	</select>
	
	<select id="isAllowed" parameterType="java.util.Map" resultType="int">
		SELECT ISALLOWED FROM REGICENTER WHERE id=#{ID} and mapkey=#{mapkey}
	</select>
	
	<update id="updateAllowed0" parameterType="java.util.Map">
		update ISALLOWED FROM REGICENTER WHERE id=#{ID} and mapkey=#{mapkey}
	</update>

</mapper>